<% provide(:title, "Character" ) %>
<div class="page-header">
    <h1>Characters<small>#index</small></h1>
</div>
    <table class="table table-hover table-bordered">
    	<tr>
    		<th>Character</th>
    	</tr>
    	<% @characters.each do |char| %>
    	<tr>
    		<td><%= link_to char, char %></td>
      		<% end %>
      	</tr>
    </table> 